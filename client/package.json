{
  "name": "npmstatistics.com",
  "version": "0.1.0",
  "private": true,
  "author": "David HÃ©rault <dherault@gmail.com> (https://github.com/dherault)",
  "license": "MIT",
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "sync-s3": "aws s3 sync build/ s3://npmstatistics.com --region eu-west-3",
    "invalidate-cloudfront": "aws cloudfront create-invalidation --distribution-id E22SULGCBGFZVG --paths /*",
    "deploy": "npm run sync-s3 && npm run invalidate-cloudfront",
    "build-deploy": "npm run build && npm run deploy"
  },
  "eslintConfig": {
    "extends": "dherault",
    "rules": {
      "no-await-in-loop": "off",
      "react/destructuring-assignment": "off"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "chart.js": "^2.8.0",
    "chartjs-color": "^2.3.0",
    "flexpad": "^0.2.1",
    "moment": "^2.24.0",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-redux": "^7.0.3",
    "react-router-dom": "^5.0.0",
    "react-scripts": "3.0.1",
    "redux": "^4.0.1"
  },
  "devDependencies": {
    "eslint-config-dherault": "^1.0.2"
  }
}
